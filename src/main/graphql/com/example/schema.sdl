type Query {
  viewer: Viewer!
}

type Viewer {
  books(input: BookInput!): [Book]!
}

type Book {
  id: String!
  name: String
  title: String,
  kind: BookKind!
}

input BookInput {
  kind: BookKind @deprecated(reason: "Not needed anymore")
  bookId: ID!
  version: Int
}

enum BookKind {
  ONE
  TWO
}